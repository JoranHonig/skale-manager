    },
    {
      "name": "Dmytro Stebaiev",
      "url": "https://github.com/DimaStebaev",
      "githubUsername": "DimaStebaev"
    }
  ],
  "scripts": {
    "compile": "rm -r ./build && ./node_modules/.bin/truffle compile",
    "deploy": "bash ./scripts/deploy.sh",
    "docs": "scripts/generate-docs.sh",
    "generate": "truffle compile && typechain --target truffle './build/**/*.json'",
    "lint": "npx ethlint --dir contracts",
    "prepare": "yarn generate",
    "test": "truffle test && yarn tsc",
    "tsc": "tsc --noEmit",
    "tslint": "npx tslint --project .",
    "slither": "slither . --exclude naming-convention,solc-version,assembly,timestamp,calls-loop,reentrancy-no-eth,constant-function,pragma --truffle-version truffle@latest"
  },
  "dependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/minimist": "^1.2.0",
    "bignumber.js": "^9.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "ethereumjs-tx": "1.3.7",
    "openzeppelin-solidity": "^2.3.0",
    "solc": "^0.5.7",
    "truffle": "^5.0.12",
    "truffle-hdwallet-provider": "^1.0.8",
    "truffle-typings": "^1.0.8",
    "ts-node": "^8.3.0",
    "typechain": "^0.3.17",
    "web3": "^1.0.0-beta.37"
  },
  "devDependencies": {
    "@types/web3": "^1.0.19",
    "codecov": "^3.6.1",
    "dotenv": "^7.0.0",
    "elliptic": "^6.5.1",
    "ethlint": "^1.2.4",
    "ganache-cli": "^6.1.8",
    "solidity-coverage": "leapdao/solidity-coverage",
    "solidity-docgen": "^0.3.4",
    "tslint": "^5.18.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "^3.5.2"
  }
}
